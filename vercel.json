{
    "version": 2,
    "build": {
        "command": "echo 'API deployment - no build needed'"
    },
    "builds": [
        {
            "src": "index.js",
            "use": "@vercel/node@22.x"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/index.js"
        }
    ],
    "env": {
        "VITE_BACKEND_URL": "https://your-server-vercel-url.vercel.app"
    }
}