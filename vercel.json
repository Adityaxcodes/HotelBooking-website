{
    "version": 2,
    "build": {
        "env": {
            "NODE_ENV": "production"
        }
    },
    "builds": [
        {
            "src": "api/index.js",
            "use": "@vercel/node@22.x"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/api/index.js"
        }
    ]
}