{
    "version": 2,
    "functions": {
        "api/index.js": {
            "runtime": "@vercel/node@22.11.0",
            "maxDuration": 30
        }
    },
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/api/index.js"
        }
    ],
    "env": {
        "NODE_ENV": "production"
    }
}